{
    "name": "aa/akeneo-data-import",
    "description": "Application for data import to Akeneo PIM",
    "type": "project",
    "authors": [
        {
            "name": "Andrey Astakhov",
            "email": "andrey.astakhov@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/flex": "^1.1@dev",
        "aa/akeneo-amqp-publisher": "dev-master",
        "aa/akeneo-fake-data-provider": "dev-master",
        "symfony/framework-bundle": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "Aa\\Akeneo\\DataImport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Aa\\Akeneo\\DataImport\\": "spec/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
